<!DOCTYPE html>
<html>
<head>
  <title>Foodws</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body class="backpic">

<%= render 'shared/headbar' %>

<div class="container posts">
  <%= yield :pathlink %>
  <div class="row">
	<div class="col-md-9">
	  <%= yield %>
    </div>

	<div class="col-md-3">
	  <%= yield :sidebar %>
    </div>
  </div>
</div>


<%= render 'shared/foot' %>
</body>
</html>

